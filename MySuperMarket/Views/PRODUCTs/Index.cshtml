        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DISCOUNT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BATCH_NUMBER)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRODUCT_ATTRIBUTE.SUPPLIER_ID)
        </td>
        <td>
