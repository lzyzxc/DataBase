}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PRODUCT_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DISCOUNT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BATCH_NUMBER)
